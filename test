echo "Hello world" > /var/www/html/index.html

#!/bin/bash
RESULT=$(find /home/ -type d -name bob)

if [ $RESULT == "/home/bob"  ]
then
        echo "<h5>1, The user bob created </h5>"   >> /var/www/html/index.html 2>/dev/null
else
        echo "<h5>0, The user bob is not created </h5> " >> /var/www/html/index.html 2>/dev/null
fi

date +%D.%M.%Y >> /var/www/html/index.html 2>/dev/null



